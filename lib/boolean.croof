def Bool = {"true", "false"}

forall a : Bool => (a) = a

def && : Bool -> Bool -> Bool

"true" && "true" = "true"
"true" && "false" = "false"
"false" && "true" = "false"
"false" && "false" = "false"

def || : Bool -> Bool -> Bool

"true" || "true" = "true"
"true" || "false" = "true"
"false" || "true" = "true"
"false" || "false" = "false"
